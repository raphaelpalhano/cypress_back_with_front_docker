    "express-session": "^1.17.1",
    "faker-br": "^0.4.1",
    "jsonschema": "^1.4.0",
    "multiple-cucumber-html-reporter": "^1.21.4",
    "rimraf": "^3.0.2",
    "swagger-autogen": "^2.21.4",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
