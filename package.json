{
  "name": "igua-auth",
  "version": "1.0.0",
  "main": "src/api/app.js",
  "license": "MIT",
  "scripts": {
    "prestart": "node swagger.js",
    "start": "NODE_ENV=production node src/api/app.js",
    "dev": "nodemon src/api/app.js",
    "test": "jest --forceExit",
    "prettier-format": "prettier --config .prettierrc '**/*.js' --write",
    "lint": "eslint . --ext .js",
    "prepare": "husky install",
    "commit": "git-cz",
    "swagger-autogen": "node swagger.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:open:prod": "cypress open --env configFile=prod",
    "cy:run:prod": "cypress run --env configFile=prod",
    "cy:tags": "cypress-tags run -e TAGS=@developed",
    "cy:report": "node ./cypress/support/report/reporter.js",
    "cy:clear": "node ./cypress/support/report/clear.js"
  },
  "dependencies": {
    "@azure/msal-node": "^1.3.2",
    "bootstrap": "^4.6.0",
    "compression": "^1.7.4",
    "cypress": "^9.6.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "swagger-autogen": "^2.21.4",
    "swagger-ui-express": "^4.4.0",
    "faker-br": "^0.4.1",
    "fs-extra": "^10.1.0",
    "jsonschema": "^1.4.0",
    "microsoft-identity-express": "azure-samples/microsoft-identity-express",
    "multiple-cucumber-html-reporter": "^1.21.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "cypress-localstorage-commands": "^1.7.0",
    "cypress-multi-reporters": "^1.5.0",
    "electron": "^19.0.6",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^8.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^13.0.2",
    "mocha": "^10.0.0",
    "mocha-junit-reporter": "^2.0.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.7.1",
    "supertest": "^6.1.4",
    "uuid": "^8.3.2"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/support/steps",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "reports/json"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier-format",
      "yarn lint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
