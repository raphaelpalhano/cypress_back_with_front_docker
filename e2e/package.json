{
  "name": "cucumber-preprocessor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prettier-format": "prettier --config .prettierrc '**/*.js' --write",
    "cy:firefox": "cypress run --browser firefox",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:developed": "cypress run -e TAGS='@developed'",
    "cy:open:developed": "cypress open -e TAGS='@developed'",
    "cy:report": "node ./cypress/support/report/reporter.js",
    "cy:clear": "node ./cypress/support/report/clear.js",
    "lint": "eslint . --ext .js --fix",
    "code:prettier": "prettier --check \"**/*.js*\""
  },
  "keywords": [],
  "author": "Raphael Angel",
  "license": "ISC",
  "devDependencies": {
    "cypress-localstorage-commands": "^1.7.0",
    "cypress-multi-reporters": "^1.5.0",
    "esbuild": "^0.14.49",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "fs": "^0.0.1-security",
    "husky": "^8.0.0",
    "lint-staged": "^12.5.0",
    "mocha-junit-reporter": "^2.0.2",
    "prettier": "^1.19.1"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/support/steps/**/*.{js,ts}",
    "cucumberJson": {
      "enabled": true,
      "outputFolder": "reports/json",
      "formatter": "/usr/local/bin/cucumber-json-formatter"
    }
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^11.5.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "cypress": "10.3.0",
    "cypress-failed-log": "^2.10.0",
    "dotenv": "^16.0.1",
    "faker-br": "^0.4.1",
    "fs-extra": "^10.1.0",
    "jsonschema": "^1.4.0",
    "multiple-cucumber-html-reporter": "^1.21.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  }
}
